<%-include("header")-%>


  <form action="/users/login" style="padding: 7% 40%;" method="POST">
    <h3><% include ./partials/messages %></h3>
    <h1>Login Here</h1>
    <div class="form-row">
      <input type="email" id="email" name="email" class="form-control" placeholder="Enter Email" autocomplete="off" />
    </div>
    <div class="form-row">
      <input type="password" id="password" name="password" class="form-control" placeholder="Enter Password" />
    </div>
    <div class="form-row">
      <button type="submit" class="btn btn-primary form-control" >
        Login
      </button>
    </div>
    <div class="form-row">
      <a href="/users/register" class="btn btn-primary form-control">Haven't Sign Up?
        Register Here </a>
    </div>
  </form>
  




  <script src="js/jquery.js"></script>
  <script src="js/bootstrap.min.js"></script>
  <script src="js/jquery.sticky.js"></script>
  <script src="js/jquery.stellar.min.js"></script>
  <script src="js/wow.min.js"></script>
  <script src="js/smoothscroll.js"></script>
  <script src="js/owl.carousel.min.js"></script>
  <script src="js/custom.js"></script>
  

  </body>

  </html>